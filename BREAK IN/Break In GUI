local a=game:GetService("ReplicatedStorage") local b=a:WaitForChild("RemoteEvents") local c=game:GetService("RunService") local d=game.Players.LocalPlayer local e=Instance.new("ScreenGui") e.Name="GuiXCD" e.Parent=d:WaitForChild("PlayerGui") local f=Instance.new("Frame") f.Name="FrmXYZ" f.Size=UDim2.new(0.3,0,0.7,0) f.Position=UDim2.new(0.35,0,0.15,0) f.BackgroundColor3=Color3.fromRGB(255,255,255) f.Parent=e local g=Instance.new("TextButton") g.Name="Btn1" g.Size=UDim2.new(1,0,0.15,0) g.Position=UDim2.new(0,0,0.85,0) g.BackgroundColor3=Color3.fromRGB(0,255,0) g.TextColor3=Color3.fromRGB(0,0,0) g.Text="Infinite Energy" g.Parent=f local h=Instance.new("TextButton") h.Name="Btn2" h.Size=UDim2.new(1,0,0.15,0) h.Position=UDim2.new(0,0,0.7,0) h.BackgroundColor3=Color3.fromRGB(0,0,255) h.TextColor3=Color3.fromRGB(255,255,255) h.Text="Give Linked Sword" h.Parent=f local i=Instance.new("TextButton") i.Name="Btn3" i.Size=UDim2.new(1,0,0.15,0) i.Position=UDim2.new(0,0,0.55,0) i.BackgroundColor3=Color3.fromRGB(255,0,0) i.TextColor3=Color3.fromRGB(255,255,255) i.Text="Cat Befriend" i.Parent=f local j=Instance.new("TextButton") j.Name="Btn4" j.Size=UDim2.new(1,0,0.15,0) j.Position=UDim2.new(0,0,0.4,0) j.BackgroundColor3=Color3.fromRGB(255,165,0) j.TextColor3=Color3.fromRGB(0,0,0) j.Text="Stealth: OFF" j.Parent=f local k=Instance.new("TextButton") k.Name="Btn5" k.Size=UDim2.new(1,0,0.15,0) k.Position=UDim2.new(0,0,0.25,0) k.BackgroundColor3=Color3.fromRGB(255,165,0) k.TextColor3=Color3.fromRGB(0,0,0) k.Text="No-Clip: OFF" k.Parent=f local l=Instance.new("TextLabel") l.Name="Lbl1" l.Size=UDim2.new(1,0,0.1,0) l.Position=UDim2.new(0,0,0.1,0) l.BackgroundColor3=Color3.fromRGB(255,255,0) l.TextColor3=Color3.fromRGB(0,0,0) l.Text="YOU HAVE TO HAVE THE TEDDIE BEAR IN YOUR INVENTORY FOR THE STEALTH TO WORK." l.TextScaled=true l.TextWrap=true l.TextXAlignment=Enum.TextXAlignment.Center l.Visible=false l.Parent=f local m=false local n, o, p f.InputBegan:Connect(function(q) if q.UserInputType==Enum.UserInputType.MouseButton1 then m=true n=q.Position p=f.Position q.Changed:Connect(function() if q.UserInputState==Enum.UserInputState.End then m=false end end) end end) f.InputChanged:Connect(function(q) if q.UserInputType==Enum.UserInputType.MouseMovement and m then local r=q.Position-n f.Position=UDim2.new(p.X.Scale,p.X.Offset+r.X,p.Y.Scale,p.Y.Offset+r.Y) end end) local s=false local t=false local u=false local function v() s=not s if s then g.BackgroundColor3=Color3.fromRGB(255,0,0) g.Text="Infinite Energy: ON" while s do b.Energy:FireServer("Cat") wait(0.1) end else g.BackgroundColor3=Color3.fromRGB(0,255,0) g.Text="Infinite Energy: OFF" end end local function w() b.GiveTool:FireServer("LinkedSword") end local function x() b.Cattery:FireServer() end local function y() l.Visible=true t=not t if t then j.BackgroundColor3=Color3.fromRGB(0,255,0) j.Text="Stealth: ON" b.MakeStealth:FireServer(13) else j.BackgroundColor3=Color3.fromRGB(255,165,0) j.Text="Stealth: OFF" b.MakeStealth:FireServer(0.05) end end local function z() u=not u if u then k.BackgroundColor3=Color3.fromRGB(0,255,0) k.Text="No-Clip: ON" c.Stepped:Connect(function() if u then for _,A in pairs(d.Character:GetDescendants()) do if A:IsA("BasePart") then A.CanCollide=false end end end end) else k.BackgroundColor3=Color3.fromRGB(255,165,0) k.Text="No-Clip: OFF" for _,A in pairs(d.Character:GetDescendants()) do if A:IsA("BasePart") then A.CanCollide=true end end end end g.MouseButton1Click:Connect(v) h.MouseButton1Click:Connect(w) i.MouseButton1Click:Connect(x) j.MouseButton1Click:Connect(y) k.MouseButton1Click:Connect(z) f.Active=true f.Draggable=true f.Selectable=false
